<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_collect_pop"
    android:orientation="vertical">

    <TextView
        android:id="@+id/share"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/dimen_26"
        android:paddingTop="@dimen/dimen_17"
        android:paddingRight="@dimen/dimen_26"
        android:paddingBottom="@dimen/dimen_17"
        android:text="分享到社区"
        android:textColor="@color/gray_33"
        android:textSize="@dimen/text_16" />

    <View
        android:id="@+id/line"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_1"
        android:layout_marginLeft="@dimen/dimen_13"
        android:layout_marginRight="@dimen/dimen_13"
        android:background="@color/gray_ef" />

    <TextView
        android:id="@+id/re_analysis"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/dimen_26"
        android:paddingTop="@dimen/dimen_17"
        android:paddingRight="@dimen/dimen_26"
        android:paddingBottom="@dimen/dimen_17"
        android:text="重新解析"
        android:textColor="@color/gray_33"
        android:textSize="@dimen/text_16"
        android:visibility="gone"
        tools:visibility="visible" />

    <View
        android:id="@+id/analysis_line"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_1"
        android:layout_marginLeft="@dimen/dimen_13"
        android:layout_marginRight="@dimen/dimen_13"
        android:background="@color/gray_ef"
        android:visibility="gone"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/revise_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/dimen_26"
        android:paddingTop="@dimen/dimen_17"
        android:paddingRight="@dimen/dimen_26"
        android:paddingBottom="@dimen/dimen_17"
        android:text="修改标题"
        android:textColor="@color/gray_33"
        android:textSize="@dimen/text_16"
        android:visibility="gone"
        tools:visibility="visible" />

    <View
        android:id="@+id/revise_title_line"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_1"
        android:layout_marginLeft="@dimen/dimen_13"
        android:layout_marginRight="@dimen/dimen_13"
        android:background="@color/gray_ef"
        android:visibility="gone"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/delete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/dimen_26"
        android:paddingTop="@dimen/dimen_17"
        android:paddingRight="@dimen/dimen_26"
        android:paddingBottom="@dimen/dimen_17"
        android:text="删除"
        android:textColor="@color/gray_33"
        android:textSize="@dimen/text_16" />


</LinearLayout>