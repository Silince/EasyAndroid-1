        <attr name="title_edit_show_left_icon" format="boolean"/>
        <attr name="title_edit_text_size" format="dimension"/>
        <attr name="title_edit_show_divider" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CleanableEditText">
        <attr name="clean_edit_text_is_mobile" format="boolean"/>
        <attr name="clean_edit_text_type" format="enum">
            <enum name="normal" value="0"/>
            <enum name="mobile" value="1"/>
            <enum name="price" value="2"/>
        </attr>
