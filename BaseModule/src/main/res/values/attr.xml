            <enum name="right" value="1"/>
        </attr>
        <attr name="item_show_divider" format="boolean"/>
        <attr name="item_show_divider_match" format="boolean"/>
        <attr name="item_show_top_divider" format="boolean"/>
        <attr name="item_show_forward" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="PasswordEditText">
        <attr name="pwd_hint" format="reference|string"/>
        <attr name="pwd_text" format="reference|string"/>
        <attr name="pwd_show_left_icon" format="boolean"/>
        <attr name="pwd_text_size" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="TitleEditText">
        <attr name="title_edit_hint" format="reference|string"/>
