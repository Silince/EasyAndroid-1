<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.chen.baseextend.view.CleanableEditText
        android:id="@+id/_edit"
        android:layout_width="@dimen/dimen_0"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_50"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="@dimen/dimen_30"
        android:layout_marginEnd="@dimen/dimen_30"/>

    <TextView
        android:id="@+id/_print"
        style="@style/user_auth_confirm_button"
        android:layout_width="@dimen/dimen_80"
        android:layout_height="@dimen/dimen_45"
        android:layout_marginTop="@dimen/text_20"
        android:text="打印"
        android:enabled="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/_edit" />

    <TextView
        android:id="@+id/_print_hq"
        style="@style/user_auth_confirm_button"
        android:layout_width="@dimen/dimen_80"
        android:layout_height="@dimen/dimen_45"
        android:layout_marginTop="@dimen/text_20"
        android:text="打印行情"
        android:enabled="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/_print" />

</androidx.constraintlayout.widget.ConstraintLayout>